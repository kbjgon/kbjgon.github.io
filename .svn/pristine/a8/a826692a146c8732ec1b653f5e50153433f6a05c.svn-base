*{
	margin:0;
	padding:0;
	border:0;
	font-family:"맑은 고딕";
	font-size:13px;
	list-style:none;
	box-sizing:border-box;
} 
/*레이아웃-상단 영역*/
.wrap{
	width:100%;
	margin:0 auto;
}
.headerwrap{
	position:fixed;
	top:0;
	left:0;
	width:100%;
	height:90px;
	background:rgba(0,0,0,.8);
}
.header{
	width:1098px;
	height:86px;
	margin:0 auto;
}
.header h1{
	display:inline-block;
	width:94px;
	height:29px;
	margin-top:30px;
	margin-left:29px;
}
.header .tnb{
	display:inline-block;
	width:133px;
	height:23px;
}
.header .tnb a{
	float:left;
	margin-left:10px;
	padding-top:5px;
	text-align:center;
	font-size:12px;
	color:white;
}
.header .nav{
	float:right;
	display:inline-block;
	width:485px;
	height:28px;
	margin-top:37px;
}
.header .nav li a{
	float:left;
	font-size:17px;
	margin-left:25px;
	padding-top:8px;
	text-align:left;
	color:white;
}
/*상단 - UI 영역*/
.visualwrap{
	width:100%;
	height:350px;
	background-image:url("../images/visual.png");
	background-repeat:no-repeat;
	background-position:50% 50%;
	background-size:cover;
}
.visual{
	position:relative;
	width:1098px;
	height:100%;
	margin:0 auto;
}
.visual p{
	position:absolute;
	bottom:85px;
	left:27px;
	color:#101e3c;
	font-size:30px;
	font-family:"Myriad Pro";
	font-weight:bold;
	text-align:center;

}
/*레이아웃 - 중간 영역*/
.submain{
	width:1098px;
	margin:0 auto;
	background:white;
}
.submain::after{
	content:"";
	display:block;
	clear:both;
}
.sidemenu{
	float:left;
	display:inline-block;
	width:274px;
	height:208px;
	padding-top:58px;
}
.sidemenu h2{
	text-align:left;
	margin-bottom:30px;
	padding-left:15px;
	color:#527fda;
	font-size:26px;
}
.sidemenu li{
	text-align:left;
}
.sidemenu li a{
	display:block;
	height:44px;
	padding:12px 16px;
	color:#555;
	font-size:14px;
	text-decoration:none;
}
.sidemenu li.on a{
	background:#5380da;
	background-image:url("../images/bul_right.png");
	background-repeat:no-repeat;
	background-position:95% 50%;
	color:#fff;
}
.section{
	position:relative;
	float:left;
	display:inline-block;
	width:824px;
	height:888px;
	padding:60px 30px;
	border-left:1px solid #ddd;
}
.section h2{
	padding-bottom:30px;
	border-bottom:1px solid #ddd;
	color:#444;
	font-size:26px;
}
/* 중단 - UI 영역 */
.location{
	position:absolute;
	right:30px;
	top:70px;
}
.location span{
	font-size:12px;
	font-family:Dotum;
}
/* 레이아웃 - 하단영역 */
.footer{
	width:100%;
	height:355px;
	background:#222;

}
.textbox{
	width:440px;
	height:230px;
	margin:0 auto;
	background:#222;
}
.textbox h2{
	font-size:18px;
	text-align:center;
	padding-top:11px;
	color:white;
}
.textbox li{
	font-size:16px;
	text-align:center;
	margin-top:37px;
	color:#666;
}
.textbox p{
	float:left;
	font-size:12px;
	text-align:center;
	margin-top:4px;
	margin-left:109px;
	color:#666
}
.textbox span{
	float:left;
	text-align:center;
	margin-top:24px;
	margin-left:50px;
	color:#999;
}
.papago{
	width:1115px;
	height:125px;
	margin:0 auto;
	background:#222;
}
.papago p{
	width:497px;
	height:35px;
	text-align:left;
	padding-top:37px;
	padding-left:9px;
	color:#717171;
}
.papago img{
	float:right;
	margin-top:51px;
	padding-right:9px;
}

